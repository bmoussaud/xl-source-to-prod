{
  "kind": "Ingress",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "front-web-ingress",
    "labels": {
      "application": "front",
      "component": "front"
    },
    "annotations": {
      "nginx.ingress.kubernetes.io/ssl-redirect": "false"
    }
  },
  "spec": {
    "rules": [
      {
        "host": "{{env}}.front.xebialabs.demo",
        "http": {
          "paths": [
            {
              "backend": {
                "serviceName": "frontend",
                "servicePort": 80
              },
              "path": "/"
            }
          ]
        }
      }
    ]
  }
}